<section>
  <%= column_chart(service_requests_by_month,
  stacked: true,
  id: id,
  xtitle: 'Month',
  ytitle: 'Total Service Requests',
  library:
  {
    title:  {
      text:  'Service Requests By Month',
      align: 'center'
    },
    chart: {
      borderWidth: 2,
      plotBackgroundColor: 'rgba(255,255,255,.9)',
      plotShadow: true,
      plotBorderWidth: 1,
      backgroundColor: '#eee'
    },
    legend: {
      align: 'right',
      verticalAlign: 'top',
      y: 75,
      x: -50,
    layout: 'vertical'
    }
  }
  ) 
  %>
</section>
