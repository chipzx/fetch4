<%= column_chart(data_series,
  stacked: is_stacked,
  id: id,
  xtitle: xtitle,
  ytitle: ytitle,
  library:
  {
    title:  {
      text:  title,
      align: 'center'
    },
    chart: {
      borderWidth: 2,
      plotBackgroundColor: 'rgba(255,255,255,.9)',
      plotShadow: true,
      plotBorderWidth: 1,
      backgroundColor: '#eee'
    },
    legend: {
      align: 'right',
      verticalAlign: 'top',
      y: 75,
      x: -50,
    layout: 'vertical'
    }
  }
  )
 %>
